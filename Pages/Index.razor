@page "/"

<PageTitle>ME2E</PageTitle>

<div class="w-100 bg-image-01">
          <div class="container">
            <div class="row py-4">
              <!--- headline --->
              <div class="col-12 px-2 pt-2 pb-4 text-center">
                <p class="fs-3 fw-light text-white mb-1">
                  Used vehicles that fit your dealership
                </p>
              </div>
              <!--- !! --->

              <!--- aut-search --->
              <div class="col-12">
                <div class="row px-1">
                  <div class="col-12 col-md-6 col-lg-4 px-1">
                    <select
                      class="selectpicker my-1"
                      id="selectpickerMakes"
                      data-width="100%"
                      multiple
                      title="Any make"
                    >
                      <option>Audi</option>
                      <option>BMW</option>
                      <option>Citroën</option>
                      <option>Fiat</option>
                      <option>Ford</option>
                      <option>Peugeot</option>
                      <option>VW</option>
                    </select>
                  </div>
                  <div class="col-12 col-md-6 col-lg-4 px-1">
                    <select
                      class="selectpicker my-1"
                      id="selectpickerFuelTypes"
                      data-width="100%"
                      multiple
                      title="Any fuel type"
                    >
                      <option>Petrol</option>
                      <option>Diesel</option>
                      <option>Electric</option>
                      <option>Hybrid</option>
                      <option>Gas</option>
                      <option>No fuel</option>
                    </select>
                  </div>
                  <div class="col-12 col-md-6 col-lg-4 px-1">
                    <select
                      class="selectpicker my-1"
                      id="selectpickerCountry"
                      data-width="100%"
                      multiple
                      title="Any country"
                    >
                      <option value="AT">Austria</option>
                      <option value="BE">Belgium</option>
                      <option value="DK">Denmark</option>
                      <option value="DE">Germany</option>
                      <option value="MX">Mexico</option>
                    </select>
                  </div>
                  <div class="col-12 px-1 py-3 text-center">
                    <button
                      class="btn btn-lg btn-primary px-5"
                      type="button"
                      onclick="window.location='search_result_list.html';"
                    >
                      Show 2,997<i class="fas fa-car-side ms-2"></i>
                    </button>
                  </div>
                </div>
              </div>
              <!--- !! --->
            </div>
          </div>
        </div>

        <!--------------------------------------------- // --------------------------------------------->

        <Auctions />
